{
  "name": "youtube-lazy-load",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "Uma biblioteca leve e de alta performance para carregar vídeos do YouTube sob demanda, melhorando a velocidade de carregamento da página.",
  "main": "./dist/youtube-lazy-load.umd.js",
  "module": "./dist/youtube-lazy-load.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/youtube-lazy-load.es.js",
      "require": "./dist/youtube-lazy-load.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "path": "^0.12.7"
  },
  "keywords": [
    "youtube",
    "lazy-load",
    "performance",
    "embed",
    "iframe",
    "vanilla-js",
    "typescript"
  ],
  "author": "Eduardo Buzzi",
  "license": "MIT"
}
